    <PreBuildEvent>@Echo off
:: Check WMIC is available
WMIC.EXE Alias /? &gt;NUL 2&gt;&amp;1 || GOTO s_error

:: Use WMIC to retrieve date and time
FOR /F "skip=1 tokens=1-6" %25%25G IN ('WMIC Path Win32_LocalTime Get Day^,Hour^,Minute^,Month^,Second^,Year /Format:table') DO (
   IF "%25%25~L"=="" goto s_done
      Set _yyyy=%25%25L
      Set _mm=00%25%25J
      Set _dd=00%25%25G
      Set _hour=00%25%25H
      SET _minute=00%25%25I
)
:s_done

:: Pad digits with leading zeros
      Set _mm=%25_mm:~-2%25
      Set _dd=%25_dd:~-2%25
      Set _hour=%25_hour:~-2%25
      Set _minute=%25_minute:~-2%25

:: Display the date/time in Invariant1 format:
Set _isodate=%25_mm%25/%25_dd%25/%25_yyyy%25
Echo %25_isodate%25 &gt; "$(ProjectDir)\Assets\BuildDate.txt"
</PreBuildEvent>
